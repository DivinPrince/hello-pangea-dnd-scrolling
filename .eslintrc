{
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "settings": {
    "ecmascript": 6
  },
  "plugins": ["react", "import"],
  "extends": ["airbnb", "plugin:prettier/recommended", "plugin:react-hooks/recommended"],
  "rules": {
    "func-names": "off",
    "no-else-return": "off",
    "no-plusplus": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
